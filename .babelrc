{
  "presets": ["next/babel"],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "~": "."
      }
    }],
    ["inline-react-svg", {
      "svgo": {
        "plugins": [
          { "removeAttrs": { "attrs": "(data-name)" } },
          { "cleanupIDs": true }
        ]
      }
    }]
  ]
}